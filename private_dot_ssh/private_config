Include ~/.orbstack/ssh/config

Host github.com gitlab.lrz.de
    User git
    PreferredAuthentications publickey
    ForwardAgent yes
    IgnoreUnknown UseKeychain
    AddKeysToAgent yes
    IdentitiesOnly yes

Host github.com
    IdentityFile ~/.ssh/id_ed25519_github

Host gitlab.lrz.de
    IdentityFile ~/.ssh/id_ed25519_gitlab_lrz